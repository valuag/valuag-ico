{
  "name": "valuag-ico",
  "private": true,
  "version": "0.0.1",
  "description": "ValuAg ICO",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "stencil build --prerender && cd functions && tsc",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"(cd functions && tsc -w)\" \"firebase serve\" \"browser-sync start --proxy \"localhost:5000\" --files \"www/*\"\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@stencil/core": "0.9.7",
    "@stencil/router": "0.2.1",
    "@stencil/sass": "0.0.5",
    "express": "4.16.3",
    "firebase": "5.0.4",
    "firebase-admin": "5.12.1",
    "firebase-functions": "1.0.4"
  },
  "devDependencies": {
    "@stencil/dev-server": "0.0.18",
    "@stencil/utils": "0.0.4",
    "@types/jest": "23.0.0",
    "browser-sync": "2.24.4",
    "firebase-tools": "3.18.6",
    "jest": "23.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valuag/valuag-ico.git"
  },
  "author": "ValuAg Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valuag/valuag-ico"
  },
  "homepage": "https://github.com/valuag/valuag-ico",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
